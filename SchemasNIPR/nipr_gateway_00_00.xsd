			<xs:element name="STATE_FEE" type="monetary_len_8_type" minOccurs="0" maxOccurs="1"/>
			<xs:element name="TRANS_FEE" type="monetary_len_8_type" minOccurs="0" maxOccurs="1"/>
			<xs:element name="TRANS_HEAD" type="trans_head_type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="TRANS_STATUS" type="trans_status_type" minOccurs="0" maxOccurs="1"/>
			<xs:element name="VALID" type="pf_type" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ____________________________________________________________________ -->
	<!-- available element
  <xs:element name="TRANS_HEAD"   type="trans_head_type" minOccurs="1" maxOccurs="1"/>
  -->
	<xs:simpleType name="transaction_type">
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="1"/>
			<!-- Appointment -->
			<xs:enumeration value="01"/>
			<xs:enumeration value="2"/>
			<!-- Termination -->
			<xs:enumeration value="02"/>
			<xs:enumeration value="3"/>
			<!-- Appointment Renewal -->
			<xs:enumeration value="03"/>
			<xs:enumeration value="4"/>
			<!-- Non-Resident license Application -->
			<xs:enumeration value="04"/>
			<xs:enumeration value="5"/>
			<!-- Non-Resident license renewal -->
			<xs:enumeration value="05"/>
			<xs:enumeration value="6"/>
			<!-- Demographics change -->
			<xs:enumeration value="06"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="trans_head_type">
		<xs:sequence>
			<xs:element name="COMMENTS" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="DATE_STAMP" type="date_type" minOccurs="1" maxOccurs="1"/>
			<!-- format : CCYYMMDD -->
			<xs:element name="STATE_CODE" type="state_code_type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="TIME_STAMP" type="time_type" minOccurs="1" maxOccurs="1"/>
			<!-- format : hhmmssxxx -->
			<xs:element name="TRANSACTION_TYPE" type="transaction_type" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
