<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<!-- Root elts -->
	<xs:element name="NIPR" type="NIPR"/>
	<xs:complexType name="NIPR">
		<xs:sequence>
			<xs:element name="ERROR" type="error_type" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="TRANSACTION" type="nipr_trans_type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ____________________________________________________________________ -->
	<!-- available element
  <xs:element name="TRANSACTION" type="nipr_trans_type"/>
  -->
	<xs:complexType name="nipr_trans_type">
		<xs:sequence>
			<xs:element name="AMENDED" type="amended_type" minOccurs="0" maxOccurs="1"/>
			<xs:element name="APPLICANT" type="applicant_type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="APPOINTER" type="appointer_type" minOccurs="0" maxOccurs="unbounded"/>
